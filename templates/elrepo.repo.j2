### Name: ELRepo.org Community Enterprise Linux Repository for el7
### URL: http://elrepo.org/

[elrepo]
name=ELRepo.org Community Enterprise Linux Repository - el7
baseurl={{ elrepo_mirror }}/elrepo/el7/$basearch/
enabled={{ elrepo_repositories.elrepo | default('0') }}
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-elrepo.org
protect=0

[elrepo-testing]
name=ELRepo.org Community Enterprise Linux Testing Repository - el7
baseurl={{ elrepo_mirror }}/testing/el7/$basearch/
enabled={{ elrepo_repositories.testing | default('0') }}
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-elrepo.org
protect=0

[elrepo-kernel]
name=ELRepo.org Community Enterprise Linux Kernel Repository - el7
baseurl={{ elrepo_mirror }}/kernel/el7/$basearch/
enabled={{ elrepo_repositories.kernel | default('0') }}
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-elrepo.org
protect=0

[elrepo-extras]
name=ELRepo.org Community Enterprise Linux Extras Repository - el7
baseurl={{ elrepo_mirror }}/extras/el7/$basearch/
enabled={{ elrepo_repositories.extras | default('0') }}
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-elrepo.org
protect=0
